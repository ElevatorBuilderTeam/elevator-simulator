<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane id="AnchorPane" prefHeight="621.0" prefWidth="883.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="elevatorsimulation.Controllers.SimulationMenuWindowController">
    <children>
        <MenuBar prefHeight="29.0" prefWidth="883.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#exitMenuOptionClicked" text="Exit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Simulation">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Run Simulation" />
                        <MenuItem fx:id="simulationEditorOption" mnemonicParsing="false" onAction="#simulationEditorOptionClicked" text="Simulation Editor" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <LineChart fx:id="elevatorChart" layoutX="279.0" layoutY="51.0" prefHeight="384.0" prefWidth="590.0">
        <xAxis>
            <CategoryAxis fx:id="timeAxis" prefHeight="27.0" prefWidth="534.0" side="BOTTOM"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="floorAxis" prefHeight="310.0" prefWidth="26.0" side="LEFT" tickLabelGap="5.0"
                        tickLength="10.0"/>
        </yAxis>
      </LineChart>
      <Button layoutX="727.0" layoutY="435.0" mnemonicParsing="false" text="View Simulation Log" />
    </children>
</AnchorPane>
